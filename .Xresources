!-- colors --!
*background:         #140c0b
*foreground:         #877a70
! black
!*color0:             #403f3e
*color0:             #242323
*color8:             #666362
! red
*color1:             #91444d
*color9:             #c78186
! green
*color2:             #6b853d
*color10:            #abbd80
! yellow
*color3:             #916949
*color11:            #cca88b
! blue
*color4:             #557282
*color12:            #8eabbd
! magenta
*color5:             #78516d
*color13:            #a8879f
! cyan
*color6:             #58756c
*color14:            #8ca8a2
! white
*color7:             #94928f
*color15:            #cdcdcd

!-- urxvt --!
urxvt.scrollBar:          false
urxvt.font:               -*-tamsynu-medium-*-*-*-14-*-*-*-*-*-*-*
urxvt.boldFont:           -*-tamsynu-bold-*-*-*-14-*-*-*-*-*-*-*
urxvt.borderLess:         false
urxvt.internalBorder:     2
urxvt.buffered:           true
urxvt.urgentOnBell:       true
urxvt.geometry:           81x21
urxvt.cursorBlink:        true
urxvt.perl-ext-common:    default,matcher
urxvt.urlLauncher:        dwb
urxvt.matcher.button:     1
urxvt.matcher.pattern.1:  \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

!-- xft --!
Xft.autohint:             0
Xft.lcdfilter:            lcddefault
Xft.hintstyle:            hintfull
Xft.hinting:              1
Xft.antialias:            1
Xft.rgba:                 rgb
